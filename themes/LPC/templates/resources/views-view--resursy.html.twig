{%
  set classes = [
    'row',
    'sources-blc', 
    'd-flex',
    'justify-content-center',
    'view-' ~ id|clean_class,
    'view-id-' ~ id,
    'view-display-id-' ~ display_id,
    dom_id ? 'js-view-dom-id-' ~ dom_id,
  ]
%}
<section class="sources">
  <div{{ attributes.addClass(classes) }}>
    {{ title_prefix }}
    {% if rows %}
      {{ rows }}
    {% elseif empty %}
      {{ empty }}
    {% endif %}
  </div>
</section>

